{
  "name": "@rafal.lemieszewski/tide-ui",
  "version": "0.87.1",
  "description": "A comprehensive React component library built with Tailwind CSS and Radix UI primitives",
  "type": "module",
  "main": "./dist/cjs/lib/index.cjs",
  "module": "./dist/es/lib/index.js",
  "types": "./dist/types/lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/lib/index.d.ts",
        "default": "./dist/es/lib/index.js"
      },
      "require": {
        "types": "./dist/types/lib/index.d.cts",
        "default": "./dist/cjs/lib/index.cjs"
      }
    },
    "./styles": {
      "import": "./dist/es/style.css",
      "require": "./dist/es/style.css",
      "default": "./dist/es/style.css"
    },
    "./chart": {
      "import": {
        "types": "./dist/types/components/fundamental/chart.d.ts",
        "default": "./dist/es/components/fundamental/chart.js"
      },
      "require": {
        "types": "./dist/types/components/fundamental/chart.d.cts",
        "default": "./dist/cjs/components/fundamental/chart.cjs"
      }
    },
    "./calendar": {
      "import": {
        "types": "./dist/types/components/fundamental/calendar.d.ts",
        "default": "./dist/es/components/fundamental/calendar.js"
      },
      "require": {
        "types": "./dist/types/components/fundamental/calendar.d.cts",
        "default": "./dist/cjs/components/fundamental/calendar.cjs"
      }
    },
    "./date-picker": {
      "import": {
        "types": "./dist/types/components/fundamental/date-picker.d.ts",
        "default": "./dist/es/components/fundamental/date-picker.js"
      },
      "require": {
        "types": "./dist/types/components/fundamental/date-picker.d.cts",
        "default": "./dist/cjs/components/fundamental/date-picker.cjs"
      }
    },
    "./country-dropdown": {
      "import": {
        "types": "./dist/types/components/fundamental/country-dropdown.d.ts",
        "default": "./dist/es/components/fundamental/country-dropdown.js"
      },
      "require": {
        "types": "./dist/types/components/fundamental/country-dropdown.d.cts",
        "default": "./dist/cjs/components/fundamental/country-dropdown.cjs"
      }
    },
    "./resizable": {
      "import": {
        "types": "./dist/types/components/fundamental/resizable.d.ts",
        "default": "./dist/es/components/fundamental/resizable.js"
      },
      "require": {
        "types": "./dist/types/components/fundamental/resizable.d.cts",
        "default": "./dist/cjs/components/fundamental/resizable.cjs"
      }
    },
    "./data-table": {
      "import": {
        "types": "./dist/types/components/product/data-table.d.ts",
        "default": "./dist/es/components/product/data-table.js"
      },
      "require": {
        "types": "./dist/types/components/product/data-table.d.cts",
        "default": "./dist/cjs/components/product/data-table.cjs"
      }
    },
    "./filters": {
      "import": {
        "types": "./dist/types/components/product/filters.d.ts",
        "default": "./dist/es/components/product/filters.js"
      },
      "require": {
        "types": "./dist/types/components/product/filters.d.cts",
        "default": "./dist/cjs/components/product/filters.cjs"
      }
    },
    "./bookmarks": {
      "import": {
        "types": "./dist/types/components/product/bookmarks.d.ts",
        "default": "./dist/es/components/product/bookmarks.js"
      },
      "require": {
        "types": "./dist/types/components/product/bookmarks.d.cts",
        "default": "./dist/cjs/components/product/bookmarks.cjs"
      }
    },
    "./linked-chart": {
      "import": {
        "types": "./dist/types/components/product/linked-chart.d.ts",
        "default": "./dist/es/components/product/linked-chart.js"
      },
      "require": {
        "types": "./dist/types/components/product/linked-chart.d.cts",
        "default": "./dist/cjs/components/product/linked-chart.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/es",
    "dist/cjs",
    "dist/types",
    "!dist/**/*.map",
    "!dist/cjs/*.css"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "rm -rf dist && vite build --config vite.config.lib.ts && node scripts/fix-cjs-types.mjs",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "storybook": "npx storybook dev -p 6006",
    "build-storybook": "npx storybook build --quiet",
    "check:publint": "publint",
    "check:types": "attw --pack . --ignore-rules=no-resolution",
    "size": "size-limit",
    "prepublishOnly": "npm run build:lib && npm run check:publint && npm run check:types",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "tailwind",
    "design-system",
    "radix-ui",
    "typescript"
  ],
  "author": "Rafal Lemieszewski",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemu/tide-ui.git"
  },
  "homepage": "https://tide-ui-storybook.vercel.app",
